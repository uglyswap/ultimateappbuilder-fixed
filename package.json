{
  "name": "ultimate-app-builder",
  "version": "1.0.0",
  "description": "Multi-agent AI system to generate production-ready apps in minutes",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "docker:build": "docker build -t ultimate-app-builder .",
    "docker:run": "docker-compose up",
    "deploy": "npm run build && npm run docker:build"
  },
  "keywords": [
    "ai",
    "app-builder",
    "code-generator",
    "multi-agent",
    "saas",
    "automation"
  ],
  "author": "Ultimate App Builder Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@prisma/client": "^5.20.0",
    "express": "^4.19.2",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.14.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "stripe": "^16.12.0",
    "@octokit/rest": "^21.0.2",
    "nodemailer": "^6.9.15",
    "ejs": "^3.1.10",
    "archiver": "^7.0.1",
    "simple-git": "^3.26.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "inquirer": "^10.2.2",
    "commander": "^12.1.0",
    "ws": "^8.18.0",
    "ioredis": "^5.4.1",
    "bullmq": "^5.13.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nodemailer": "^6.4.16",
    "@types/archiver": "^6.0.2",
    "@types/ws": "^8.5.12",
    "typescript": "^5.6.2",
    "tsx": "^4.19.1",
    "tsc-alias": "^1.8.10",
    "prisma": "^5.20.0",
    "vitest": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.11.1",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "prettier": "^3.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
